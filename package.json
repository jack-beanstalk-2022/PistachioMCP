{
  "name": "pistachio-mcp",
  "version": "1.0.0",
  "description": "MCP server for mobile app development",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "dev": "FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIRESTORE_EMULATOR_HOST=localhost:8080 tsx watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "mcp:start": "pm2 start ecosystem.config.cjs",
    "mcp:stop": "pm2 stop pistachio-mcp",
    "mcp:restart": "pm2 restart pistachio-mcp",
    "mcp:logs": "pm2 logs pistachio-mcp",
    "mcp:status": "pm2 status",
    "mcp:delete": "pm2 delete pistachio-mcp",
    "rclone:start": "pm2 start ecosystem.config.cjs --only rclone-webdav",
    "rclone:stop": "pm2 stop rclone-webdav",
    "rclone:restart": "pm2 restart rclone-webdav",
    "rclone:logs": "pm2 logs rclone-webdav",
    "rclone:status": "pm2 describe rclone-webdav",
    "rclone:delete": "pm2 delete rclone-webdav",
    "proxy:start": "pm2 start ecosystem.config.cjs --only webdav-proxy",
    "proxy:stop": "pm2 stop webdav-proxy",
    "proxy:restart": "pm2 restart webdav-proxy",
    "proxy:logs": "pm2 logs webdav-proxy",
    "proxy:status": "pm2 describe webdav-proxy",
    "proxy:delete": "pm2 delete webdav-proxy"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "firebase": "^12.8.0",
    "pino": "^10.3.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "pm2": "^6.0.14",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.10.3"
}
